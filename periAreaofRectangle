#include <stdio.h>
#include <conio.h>

int main()
{
    int perimeter, area, height, width;
    height = 7;
    width = 5;
    
     perimeter = height + height + width + width;
     printf("The perimeter of rectangle is %d\n", perimeter);
     
     area = height * width;
     printf("The area of rectangle is %d", area);
    return 0;
}