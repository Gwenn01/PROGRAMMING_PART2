import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s =  new Scanner(System.in);
		System.out.print("Enter a number: ");
		int n = s.nextInt();
		boolean isNotPrime = false;
		
		if(n == 1){
			isNotPrime = true;		
		}else{
		     for(int i = 2; i < n; i++){
		          if(n%i == 0){
		          	isNotPrime = true;
		          	break;
		       	}	
		       	
		  	}	
		}
		
	if(isNotPrime){
	    System.out.print(n + " is Not!!! Prime number");
	}else {
		System.out.print(n + " is Prime number");
	}	
		
		
	}
}