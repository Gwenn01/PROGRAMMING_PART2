import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
		System.out.print("Enter a one digits number");
		int n = s.nextInt();
		
		int a, b, c;
		a = n;
		b = n+n;
		c = n+n+n;
		
		System.out.println("n = " + a);
		System.out.println("n + n = " + b);
	    System.out.println("n + n + n = " + c);
		
	}
}