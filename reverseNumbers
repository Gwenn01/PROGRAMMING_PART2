import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.print("Enter a number: ");
		int n = s.nextInt();
		int rem;
		String result = "";
		while(n != 0){
			rem = n % 10;
			result = result + rem;
			n /= 10;
		}
		
		int num = Integer.parseInt(result);
		
		System.out.print("The reverse of number is " + num);
	}
}