import java.util.*;
public class Main {
	public static void main(String[] args) {
		int[] arr = {5, 2, 7, 8, 1};
		int temp = 0;
	
		for(int i = 0; i <= arr.length-1; i++){
			for(int j = 0+1;j <= arr.length-1; j++ ){
				if(arr[i] > arr[j]){
					temp = arr[i];
					arr[i] = arr[j];
					arr[j] = temp;
				}
			}		
		}
		
	for(int i = 0; i <= arr.length-1; i++)
		{
		System.out.print(arr[i]+" ");	
		}
	
	}
}
}