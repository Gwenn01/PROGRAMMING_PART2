import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.print("Enter N: ");
		int n = s.nextInt();
		
		for(int i = 0; i <= n; i++){
		     for(int j = 0; j <= n*2; j++){ 
		     if(i == 0 || i == n || j == 0 || j == n*2){
	          System.out.print("*");
	            
    	     }else {
		     	System.out.print(" "); 
		       }
		     }
			System.out.println();	
		}
	}
}