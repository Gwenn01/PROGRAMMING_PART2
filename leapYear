import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.print("Enter a year: ");
		int year = s.nextInt();
		
		if((year % 400 == 0) || (year % 4 == 0 && year % 100 != 0)){
			System.out.print("Its a Leap Year");
		}else {
			System.out.print("Its not a Leap Year");
		}
	}
}